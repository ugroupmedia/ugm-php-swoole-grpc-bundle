{
    "name": "ugroupmedia/swoole-grpc-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Provides gRPC+Protobuf support in Symfony.",
    "require": {
        "php": "^7.3 || ^8.0",
        "k911/swoole-bundle": "^0.8",
        "spiral/roadrunner-grpc": "^2",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/event-dispatcher": "^4.0 || ^5.0",
        "symfony/http-foundation": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5"
    },
    "bin": [
        "bin/generate-contract.sh"
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Ugm\\SwooleGrpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ugm\\SwooleGrpc\\Tests\\": "tests/"
        }
    }
}
