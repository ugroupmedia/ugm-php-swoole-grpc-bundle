{
    "name": "ugroupmedia/swoole-grpc-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Provides gRPC+Protobuf support in Symfony.",
    "require": {
        "php": "^7.3",
        "k911/swoole-bundle": "^0.7.5",
        "spiral/php-grpc": "^1.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/event-dispatcher": "^4.0 || ^5.0",
        "symfony/http-foundation": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Ugm\\SwooleGrpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ugm\\SwooleGrpc\\Tests\\": "tests/"
        }
    }
}
